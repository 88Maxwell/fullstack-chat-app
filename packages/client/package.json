    "axios": "^1.6.1",
    "clsx": "^2.0.0",
    "connected-react-router": "^6.9.3",
    "consola": "^3.2.3",
    "date-fns": "^2.30.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "react-router-dom": "5.2.0",
    "redux": "^4.2.0",
    "sass": "^1.69.5",
    "socket.io-client": "^4.7.2",
